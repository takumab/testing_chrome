{

  "manifest_version": 2,

  "name": "True AI",
  "description": "True AI extension enables automated human conversations in customer support.",
  "version": "1.0",

  "browser_action": {
    
  },

  "content_scripts": [
    {
    "matches": [
      "<all_urls>"
      ],
      "js": ["jquery-3.1.0.js", "content.js"]
    }

  ],

  "background": {
    "scripts": ["background.js"]

  },

  "permissions": [
    "activeTab", 
    "https:"
  ]

}